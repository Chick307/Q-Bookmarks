{
	"name": "Q Bookmarks",
	"version": "1.2.3",
	"description": "Bookmark Popup",
	
	"icons": {
		"16": "image/icon-16.png",
		"48": "image/icon-48.png",
		"128": "image/icon-128.png"
	},
	
	"permissions": [
		"tabs",
		"*://*/*",
		"bookmarks",
		"chrome://favicon/"
	],
	
	"background_page": "background.html",
	"options_page": "views/option.html",
	
	"browser_action": {
		"default_title": "Q Bookmarks",
		"default_icon": "image/icon-19.png",
		"popup": "views/frame.html"
	}
}
